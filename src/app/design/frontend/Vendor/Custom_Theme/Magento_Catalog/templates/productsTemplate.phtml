<div class="productsTemplate"></div>
<script type="text/x-magento-init">
    {
        ".productsTemplate": {
            "Magento_Catalog/js/underscore-template": {}
        }
    }
</script>
<script class="custom-js-template" type="text/x-magento-template">
    <h1><%- title %></h1>

    <ul class="productsList">
        <% _.each(products, product => { %>
            <li class="productName">
                <%- product.name %>
                <br />
                <%- product.qty %>
            </li>
        <% }); %>
    </ul>
    
</script>